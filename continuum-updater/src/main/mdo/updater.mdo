<model>
  <id>continuum-updater</id>
  <name>ContinuumUpdater</name>
  <description>Continuum updater's object model.</description>
  <defaults>
    <default>
      <key>package</key>
      <value>org.apache.maven.continuum.updater.model</value>
    </default>
  </defaults>

  <classes>
    <class rootElement="true">
      <name>UpdaterModel</name>
      <version>1.0.0+</version>
      <fields>
        <field>
          <name>versions</name>
          <version>1.0.0+</version>
          <association>
            <type>Version</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
      </fields>
    </class>
    <class>
      <name>Version</name>
      <version>1.0.0+</version>
      <fields>
        <field>
          <name>name</name>
          <version>1.0.0+</version>
          <type>String</type>
        </field>
        <field>
          <name>componentRole</name>
          <version>1.0.0+</version>
          <type>String</type>
        </field>
      </fields>
    </class>
  </classes>
</model>
