<form>
  <extend>continuumProject</extend>
  <id>maven-1Project</id>
  <target-class>org.apache.maven.continuum.project.MavenOneProject</target-class>
  <lookup-expression>getMavenOneProject(#id)</lookup-expression>
  <elements>
    <element>
      <id>project.pomUrl</id>
      <validator>default</validator>
      <message-key>project.pomUrl.message</message-key>
      <label-key>project.pomUrl.label</label-key>
      <error-message-key>project.pomUrl.error</error-message-key>
      <type>text</type>
    </element>
  </elements>
  <add>
    <title-key>project.title</title-key>
    <action>addMavenOneProject</action>
    <view>projectOverview.vm</view>
  </add>
</form>
